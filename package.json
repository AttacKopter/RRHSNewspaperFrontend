{
  "name": "rrhsnewsstand",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend for the RRHS Newstand a revitalized news outlet for Reagan IB students",
  "type": "module",
  "scripts": {
    "client": "vite --port 4000 --host --https --clearScreen false",
    "server": "nodemon ./src/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test-watch": "npm run test -- --watchAll",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AttacKopter/RRHSNewsstandFrontend.git"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "express": "^4.19.2",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "react-router": "^6.22.3",
    "sass": "^1.75.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "vite": "^5.2.0"
  },
  "license": "ISC",
  "homepage": "https://github.com/AttacKopter/RRHSNewsstandFrontend#readme"
}
